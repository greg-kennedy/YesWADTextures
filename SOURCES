TARGETNAME=YesWADTextures
TARGETTYPE=PROGRAM

UMTYPE=console
UMENTRY=main

SOURCES=bsp.c texture.c utils.c wad.c main.c
USE_MSVCRT=1

386_STDCALL=2

MSC_OPTIMIZATION=/O2
LINK_TIME_CODE_GENERATION=1
MSC_WARNING_LEVEL=/W4
BUILD_ALLOW_ALL_WARNINGS=1
USER_C_FLAGS=/D"_CONSOLE" /TP

LINKER_FLAGS=/INCREMENTAL:NO /MANIFEST:NO /SUBSYSTEM:CONSOLE /OPT:REF /OPT:ICF /MACHINE:X86
